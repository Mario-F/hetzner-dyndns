  "version": "0.2.0",
  "configurations": [
    {
      "name": "External debugging",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "${workspaceFolder}",
      "port": 8345,
      "host": "127.0.0.1"
    },
    {
      "name": "Launch file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": ["myip"]
    }
  ]
}
